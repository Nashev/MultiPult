  WaveUtils in 'WaveUtils.pas',
  WaveIn in 'WaveIn.pas',
  WaveIO in 'WaveIO.pas',
  WaveOut in 'WaveOut.pas',
  WavePlayers in 'WavePlayers.pas',
  WaveRecorders in 'WaveRecorders.pas',
  WaveRedirector in 'WaveRedirector.pas',
  WaveReg in 'WaveReg.pas',
  WaveStorage in 'WaveStorage.pas',
  WaveTimer in 'WaveTimer.pas',
  WaveMixer in 'WaveMixer.pas';

end.
